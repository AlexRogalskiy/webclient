{
  "name": "ctemplar",
  "version": "2.3.28",
  "license": "Apache",
  "main": "electron-main.js",
  "description": "Angular webclient (with Linux, macOS and Windows desktop clients) for CTemplar's encrypted email service.",
  "author": {
    "name": "Templar Software Systems Ltd.",
    "email": "support@ctemplar.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve -c local",
    "start:local:ssl": "ng serve -c local --ssl",
    "start:electron": "electron .",
    "build": "ng build --prod --subresource-integrity",
    "build:dev": "ng build --output-hashing all  --subresource-integrity",
    "build:electron": "tsc -p electron-tsconfig.json && ng build --prod --subresource-integrity --base-href ./ && rm electron-main.js*",
    "build:electron:start": "npm run build:electron && electron .",
    "build:electron:linux": "npm run build:electron && electron-builder build --linux",
    "build:electron:windows": "npm run build:electron && electron-builder build --windows",
    "build:electron:mac": "npm run build:electron && electron-builder build --mac",
    "build:ipfs": "ng build --prod --subresource-integrity --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc && node patch-webpack.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/localize": "^9.1.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "@ngneat/until-destroy": "^7.1.1",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sentry/browser": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "cssfilter": "^0.0.10",
    "electron-updater": "^4.3.1",
    "juice": "^7.0.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.27",
    "ngx-chips": "^2.1.0",
    "ngx-quill": "^8.0.0",
    "openpgp": "^4.10.4",
    "quill": "^1.3.7",
    "quill-image-resize-module": "^3.0.0",
    "raven-js": "^3.27.2",
    "rxjs": "^6.5.4",
    "simple-keyboard": "^2.29.50",
    "tslib": "^1.11.1",
    "xss": "^1.0.7",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.47",
    "codelyzer": "^5.1.2",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "node-sass": "^4.13.1",
    "protractor": "^5.4.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "~3.8.3"
  }
}
