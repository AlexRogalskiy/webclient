{
  "name": "ctemplar",
  "version": "2.3.18",
  "license": "Apache",
  "main": "electron-main.js",
  "description": "Angular webclient (with Linux, macOS and Windows desktop clients) for CTemplar's encrypted email service.",
  "author": {
    "name": "Templar Software Systems Ltd.",
    "email": "support@ctemplar.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:electron": "electron .",
    "serve": "ng serve",
    "build": "ng build --prod --subresource-integrity",
    "build:dev": "ng build --output-hashing all  --subresource-integrity",
    "build:electron": "tsc -p electron-tsconfig.json && ng build --prod --subresource-integrity --base-href ./",
    "build:electron:start": "npm run build:electron && electron .",
    "build:electron:linux": "npm run build:electron && electron-builder build --linux",
    "build:electron:windows": "npm run build:electron && electron-builder build --windows",
    "build:electron:mac": "npm run build:electron && electron-builder build --mac",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
