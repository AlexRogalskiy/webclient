    "@angular/compiler-cli": "9.0.5",
    "@angular/language-service": "9.0.6",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^8.2.1",
    "electron-packager": "^14.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
