{
  "name": "ctemplar",
  "version": "2.3.3",
  "license": "Apache",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "build": "ng build --prod --subresource-integrity",
    "dev": "ng build --output-hashing all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "start:electron": "tsc --p electron && electron .",
    "build:electron": "tsc --p electron",
    "build-all": "ng build --prod --subresource-integrity --base-href ./ && tsc --p electron",
    "build-all-dev": "ng build --base-href ./ && tsc --p electron",
    "pack": "electron-packager . CTemplar --out=electron/clients --overwrite --ignore=node_modules --ignore=docs --ignore=.idea --ignore=.github --ignore=src --ignore=e2e",
    "pack-all": "./set-electron-base-href.sh && electron-packager . CTemplar --all --out=electron/clients --overwrite --ignore=node_modules --ignore=docs --ignore=.idea --ignore=.github --ignore=src --ignore=e2e && ./revert-electron-base-href.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.6",
    "@angular/common": "9.0.6",
    "@angular/compiler": "9.0.6",
    "@angular/core": "9.0.6",
    "@angular/forms": "9.0.6",
    "@angular/localize": "^9.0.5",
    "@angular/cdk": "^9.2.2",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "@ngneat/until-destroy": "^7.1.1",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "cssfilter": "0.0.10",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.27",
    "ng-keyboard-shortcuts": "^8.2.5",
    "ngx-chips": "^2.1.0",
    "ngx-quill": "^8.0.0",
    "openpgp": "^4.8.0",
    "quill": "^1.3.7",
